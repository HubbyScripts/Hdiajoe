shared.stop = true -- stops the player at any time if true

-- CONFIG:

shared.ftime = 55 -- time in seconds for the song to finish (extended by |)
shared.delay = nil -- delay overides the ftime
shared.tempo = nil -- tempo overides the delay

shared.scr = [[ 
p a s s d a p o | p p a s p o h h d | p p a s p s d a p o | p p a s p o d d d f d
s d f s d d d f d o | p a s p d f d
o p s p f f d | o p s p d d s | o p s p s d a p o o d s
o p s p f f d | o p s p h a s a p | o p s p s d a p o o d s
s s p s p s d | a p o | p p a s p o h | h d | p p a s p s d | a p o | p p a s p o | d d d f d
s d f s d d d f d o | p a s p d f d
o p s p f f d | o p s p d d s | o p s p s d a p o o d s
o p s p f f d | o p s p h a s a p | o p s p s d a p o o d s ]]

loadstring(game:HttpGet("https://raw.githubusercontent.com/Nameless9000/RobloxPianoAutoplayer/main/script.lua"))()
